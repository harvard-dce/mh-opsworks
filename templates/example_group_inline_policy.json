{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "",
            "Effect": "Allow",
            "Action": [
                "cloudformation:*",
                "ec2:*Tags*",
                "ec2:*Vpc*",
                "ec2:*Subnet*",
                "ec2:*Gateway*",
                "ec2:*Vpn*",
                "ec2:*Route*",
                "ec2:*Address*",
                "ec2:*SecurityGroup*",
                "ec2:*NetworkAcl*",
                "ec2:*DhcpOptions*",
                "ec2:RunInstances",
                "ec2:StopInstances",
                "ec2:StartInstances",
                "ec2:TerminateInstances",
                "ec2:Describe*",
                "ec2:ModifyInstanceAttribute",
                "ec2:*NetworkInterface*",
                "iam:*InstanceProfile*",
                "iam:*Polic*",
                "iam:*Role*",
                "iam:*User*",
                "sns:*",
                "cloudwatch:*",
                "logs:*",
                "s3:*",
                "elasticfilesystem:*",
                "rds:describe*",
                "rds:createDBSubnetGroup",
                "rds:deleteDBSubnetGroup",
                "rds:createDBInstance",
                "rds:deleteDBInstance",
                "rds:createEventSubscription",
                "rds:modifyEventSubscription",
                "rds:deleteEventSubscription"
            ],
            "Resource": [
                "*"
            ]
        }
    ]
}
